class  Narrowing1
{
	public static void main(String[] args) 
	{
		double a=57.89;
		float b=(float)a;
		long c=(long)a;
		int d=(int)a;
		short e=(short)a;
		byte f=(byte)a;
        char g=(char)a;
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);
		System.out.println(g);
	}
}
