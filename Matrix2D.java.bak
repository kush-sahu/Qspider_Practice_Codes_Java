class Matrix2D 
{
	public static void main(String[] args) 
	{

		int mat[][]={{1,3,5,7},{10,11,16,20},{23,30,34,60}};
		int target=34;

		int r=0;
		int c=mat[0].length-1;
         
		 while((r>=0 && r<mat.length) && (c>=0 && c<mat[0].length)){
			 if(mat[r][c]==target)System.out.println(r +" "+ c);

			 else if(mat[r][c]>target){
				 r--;
			 }else c++;
		 }
	} 
}
